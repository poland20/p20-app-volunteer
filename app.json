{
  "expo": {
    "name": "Volunteer 2.0 Summit",
    "slug": "p20-app-volunteer",
    "privacy": "public",
    "sdkVersion": "35.0.0",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "buildNumber": "3",
      "bundleIdentifier": "com.poland20volunteer.app",
      "supportsTablet": false
    },
    "android": {
      "package": "com.poland20volunteer.app",
      "versionCode": 3,
      "permissions": [
        "ACCESS_COARSE_LOCATION",
        "ACCESS_FINE_LOCATION",
        "CAMERA",
        "INTERNET",
        "ACCESS_NETWORK_STATE"
      ]
    }
  }
}